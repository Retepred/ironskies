<br>
<div class='main2'>
  <div class="whitebackground">
  </div>
  <div class='textbox'>
    <tbody>
      <div class='newgamebutton'><%= link_to 'New Game', new_game_path(@game) %></div>
      <br>
      <center><p><b>List of Games</b></p></center>
      <% @games.each do |game| %>
      <tr>
        <div>
          <a class='gamelistbutton' href=<%= game_path(game) %>><%=game.name%></a>
        </div>
      </tr>
      <% end %>
    </tbody>
  </div>
</div>